
node ../ebControl.js --specialty --forReal dataFiles/eb/specialtyExperiment/specialties.eb
		if [ $? -eq 1 ]
		then
			echo "found error";
			exit 1;
		fi

node ../ebControl.js --objective  --forReal dataFiles/eb/specialtyExperiment/objectives.eb
		if [ $? -eq 1 ]
		then
			echo "found error";
			exit 1;
		fi
node ../ebControl.js --assignSpecialty  --forReal dataFiles/eb/specialtyExperiment/specialtyAssignments.eb
		if [ $? -eq 1 ]
		then
			echo "found error";
			exit 1;
		fi

node ../ebControl.js --homeroom --skipFirstLine --forReal -- dataFiles/eb/specialtyExperiment/homeroom.eb
		if [ $? -eq 1 ]
		then
			echo "found error";
			exit 1;
		fi
node ../ebControl.js --assignStudent --forReal dataFiles/eb/specialtyExperiment/assignStudent.eb
		if [ $? -eq 1 ]
		then
			echo "found error";
			exit 1;
		fi
node ../ebControl.js --assignTeacher --forReal dataFiles/eb/specialtyExperiment/assignTeacher.eb
		if [ $? -eq 1 ]
		then
			echo "found error";
			exit 1;
		fi

echo "Specialty Experiment upload completed successfully"